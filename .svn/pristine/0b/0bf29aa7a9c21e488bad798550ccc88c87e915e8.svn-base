	background: rgba(0,0,0,.5);
	position: absolute;
	left:100%;
	top:0;
	z-index: 10001;
}
.fgoodslist{
	width: 88%;
	height: 100%;
	background: #fff;
	position: absolute;
	right:0;
	z-index: 10005;
}

.fgoodslistT{
	width: 100%;
	height: 2.8rem;
	line-height: 2.8rem;
	padding-left:3%;
	border-bottom:1px solid #D2D2D2;
	position: relative;
}
.fgoodsClose{
	position: absolute;
	right:0.5rem;
	
}
.fgoodsClose img{
	width:50%;
}
.goodslistitem{
	width: 100%;
	overflow: hidden;
}
.epadding{
	padding:5% 0 5% 2%;
	height: 6rem;
	
}
.fstores{
	background-color:#F2F2F2 ;
	line-height: 2.2rem;
	padding-left:2%;
}
.eleftImg{
	width: 30%;
	height: 6rem;
