	margin-top:1%;
	
}
#goodsImg{
	padding-left:2%;
}
.nogoods{
	width: 1.7rem;
	height: 1.7rem;
	border-radius: 50%;
	-webkit-border-radius: 50%;
	position: absolute;
	top:40%;
	left:26%;
	margin-top:-3%;
	background: rgba(0,0,0,.5);
	line-height: 1.7rem;
	text-align: center;
	color:#fff;
	font-size:0.6rem;
	transform: rotateZ(-30deg);
	-webkit-transform: rotateZ(-30deg);
	-moz-transform: rotateZ(-30deg);
	-ms-transform: rotateZ(-30deg);
	-khtml-transform: rotateZ(-30deg);
	-o-transform: rotateZ(-30deg);
}

.description{
	height: 100%;
	position: absolute;
	right:10%;
	top:0%;
}
.goodsinfomation{
	line-height: 4rem;
}
.fsend{
	background-color: #FFFFFF;
}
.fpayStyle{
	height: 3rem;
	position: relative;
	border-bottom: 1px solid #d2d2d2;
	
}
.paytip1{
	line-height: 3rem;
	padding-left:2%;
}
.onlinePay{
	/*margin-top:-2.5%;*/
	line-height: 2.8rem;
}
.sendStyle,.billinfo{
	height: 5rem;	
	position: relative;
	padding-left:2%;
	border-bottom: 1px solid #d2d2d2;
}
.sendStyle{
	height: 5rem;
}
.paytip2{
	line-height: 5rem;
}
.delivery{
	line-height: 1.6rem;
	top:18%;
}

.billinfo{
	margin-bottom: 3%;
	height: 6rem;
}
.paytip3{
	line-height:6rem;
}
.bill{
	top:15%;
}
.bill p{
	height:1.6rem;
	
}

/*备注*/
.fremarks{
	margin-bottom: 3%;
	width: 100%;
	height: 5rem;
	background: #fff;
	padding-left:2%;
}
.remarkname{
	line-height: 2rem;
}
.resize{
	border: 1px solid #D2D2D2;	
	resize: none;
	width: 93%;
	height: 1.8rem;
	line-height: 1.8rem;
	padding-left:2%;
	outline: none;
}
.goodsTall{
	margin-bottom: 10%;
	width: 98%;
	height: 3rem;
	background: #fff;
	padding:3% 0 3% 2%;
	border-bottom:1px solid #D2D2D2;
}
.sendPrice{
	float:right;
	line-height: 1.5rem;
	margin-right:3%;
	color:#333333;
}
.goodsTallN{
	line-height: 1.5rem;
}

.fshouldpay{
	width: 100%;
	height: 2.5rem;
	position: fixed;
	bottom:0;
	right:0;
	z-index: 10003;
	text-align: right;
	line-height: 2.5rem;
	background-color:#F9F9F9 ;
}
.shouldPayM{
	color:#FF0000;
	font-size:0.9rem;
}
.fsbmit{
	display: inline-block;
	height: 100%;
	background-color:#b91b34;
	color:#fff;
	text-align: center;
	padding:0 3%;
}

