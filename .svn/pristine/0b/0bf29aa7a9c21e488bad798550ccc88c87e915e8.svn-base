}
.colorbottom{
	width: 100%;
	height: 0.2rem;
	position:absolute;
	bottom: -2%;
	left:1%;
	overflow: hidden;
}
.colorbottom span{
	height: 100%;
	display: inline-block;
	transform: skew(45deg,0deg);
	-webkit-transform: skew(45deg,0deg);
	-moz-transform: skew(45deg,0deg);
	-ms-transform:skew(45deg,0deg) ;
	-o-transform:skew(45deg,0deg) ;
	-khtml-transform:skew(45deg,0deg) ;
	position: absolute;
	bottom:0;
}
