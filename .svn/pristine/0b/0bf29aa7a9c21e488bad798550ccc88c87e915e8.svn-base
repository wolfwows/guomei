html,body{
	width: 100%;
	height: 100%;
	color:#999999;
	position: relative;
	
}
.editOrderCon{
	width: 100%;
	padding-bottom: 2%;
	border-bottom: 1px solid #D2D2D2;
	overflow: hidden;
}
.editAdress{
	width: 100%;
	height: 6rem;
}
.frightArow{
	float: right;
	font-size:1.2rem;
