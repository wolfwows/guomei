#shoppingCart1_wrap {
	background-color: rgb(236, 236, 236);
	position: relative;
}

#shoppingCart1_wrap .self_first {
	width: 100%;
	position: relative;
	overflow: hidden;
	font-size: 14px;
	background-color: #FFFFFF;
}
.shoppingCart1_checkBox {
	width: 5%;
	padding: 10px;
	float: left;
	/*background-color: chartreuse;*/
	/*position: relative;*/
}

.self_list .shoppingCart1_checkBox {
	padding: 40px 10px;
}
.self_wBG {
	/*background-color: #fff;*/
	width: 18px;
	height: 18px;
	border-radius: 19px;
	border: 1px solid lightgray;
	/*background-image: url(../img/shoppingCart1/shopCart_Checked.png);*/
	background-size:100% 100% ;
}


.self_first span {
	position: absolute;
	left: 40px;
	top: 13px;
}
.self_first a {
	color: gray;
	right: 10px;
	top: 13px;
	position: absolute;
}
.self_list {
	display: inline-block;
	width: 100%;
	position: relative;
	overflow: hidden;
	/*height: 150px;*/
	background: rgb(247,247,247);
}
.self_image {
	width: 72px;
	position: absolute;
	left: 40px;
	top: 13px;
	float: left;
	background-color: gold;
}

.self_image img {
	width: 100%;
}

.self_text {
	width: 235px;
	float: right;
	margin-right: 5px;
	margin-top: 15px;
	overflow: hidden;
	font: "agency fb";
}

.self_list span {
	display: inline-block;
}

.self_list span:nth-child(1) {
	color: gray;
	font-size: 14px;
}

.self_list span:nth-child(3) {
	color: lightgray;
	font-size: 12px;
	margin-top: 6px;
}
.self_list span:nth-child(5) {
	color: red;
	font-size: 12px;
}
.self_list span:nth-child(6) {
	color: red;
	margin-top: 10px;
}
.self_list span:nth-child(7) {
	color: red;
	font-size: 12px;
}

.self_list .self_image .self_residue span {
	position: absolute;
	display: inline-block;
	width: 100%;
	font-size: 8px;
	text-align: center;
	background-color: #E43643;
	color: #fff;
	bottom: 0;
	padding: 3px 0;
}

.self_select {
	position: relative;
	width: 38px;
	float: right;
}

.self_select span {
	font-size: 0.5rem;
	display: inline-block;
	padding: 3px 20px 2px 4px;
	background-color: white;
	border-radius: 3px;
	border: 1px solid lightgray;
}

.self_select .self_number {
	width: 8px;
	position: absolute;
	right: 8px;
	top: 1px;
}

.self_select .self_number img {
	width: 100%;
}

#invalid_goods {
	width: 100%;
	height: 80px;
	position: relative;
}
#invalid_goods input {
	border: 1px solid lightgray;
	border-radius: 3px;
	width: 100px;
	height: 30px;
	text-align: center;
	color: rgb(236,164,101);
	position: absolute;
	top: 0;
	left: 0;
	right: 0;
	bottom: 0;
	margin: auto;
}

#shoppingCart1_balance {
	background-color: rgb(247,247,247);
	position: relative;
	overflow: hidden;
	height: 50px;
}

#shoppingCart1_balance>span:nth-child(2) {
	position: absolute;
	display: inline-block;
	font-size: 8px;
	color: darkgray;
	margin: 15px 20px 0 3px;
}

#shoppingCart1_balance .balance_right {
	float: right;
	margin-top: 5px;
}
#shoppingCart1_wrap #shoppingCart1_balance input {
	float: right;
	width: 90px;
	height: 40px;
	background-color: rgb(168,44,58);
	color: #fff;
}
.balance_right span:nth-child(1) {
	color: darkgray;
	font-size: 14px;
}
.balance_right span:nth-child(2) {
	color: mediumvioletred;
	font-size: 14px;
}
.balance_right span:nth-child(3), span:nth-child(5){
	color: darkgray;
	font-size: 12px;
}
.balance_right span:nth-child(5) {
	float: right;
	margin-top: 3px;
}

