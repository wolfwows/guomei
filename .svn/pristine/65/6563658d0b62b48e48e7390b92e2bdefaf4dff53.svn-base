$(function(){
	var flag=true;
	$(".fbgred").on("click",function(){
		if(flag){
		$(this).css({"backgroundColor":"#b91b34"});
		$(this).find(".fbgWriteIn").css({"left":"30px"});
		flag=false;
	}else{
		$(this).css({"backgroundColor":"#FFFFFF"});
		$(this).find(".fbgWriteIn").css({"left":"1px"});
		flag=true;
	}
	});
	
})
