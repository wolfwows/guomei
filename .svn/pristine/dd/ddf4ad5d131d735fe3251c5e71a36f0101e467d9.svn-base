html,body{
	width: 100%;
	height: 100%;
}
.fgetAdressCon{
	width: 94%;
	margin:0 auto;
	padding-top:2%;
}
.addressItem{
	background: #FFFFFF;
	margin-bottom:3% ;
}
.adressCom{
	/*height: 5rem;*/
	
	padding:5% 0;
	display: flex;
	justify-content: space-between;
	border-bottom: 1px solid #D2D2D2;
}

.ischecked{
	width: 10%;
	height: 100%;
	
	position: relative;
}
.gtext{
	width: 90%;
}
.guseInfo{
	color:#333333;
	line-height: 1.5rem;
}
.guserPhone{
	padding-left:5%;
}
/*默认按钮*/
.gapprove{
	display: inline-block;
	width: 12%;
	border:1px solid red;
	height: 0.9rem;
	line-height: 0.9rem;
	text-align: center;
	border-radius: 3px;
	-webkit-border-radius: 3px;
	color:#F09B2B;
	font-size:0.6rem;
	display: none;
}

.gadressCon{
	color:#777777;
	line-height: 1.4rem;
	font-size:0.8rem;
}

.geidtaddress{
	height: 2rem;
	font-size:0.8rem;
	color:#777777;
	line-height: 2rem;
	position: relative;
}
.eidt{
	display: inline-block;
	width: 20%;
	height: 100%;
	float:right;
	text-align:center;
}
.eidt img{
	width: 1rem;
	vertical-align: middle;
}

.eidtNav{
	width: 22%;
	border:1px solid #D2D2D2;
	background: #FFFFFF;
	position: absolute;
	bottom:2rem;
	right:0;
	text-align: center;
	display: none;
}
.eidtNav ul .item5{
	color:#E5A782;
	height: 1.6rem;
	line-height: 1.6rem;
	font-size:0.8rem;
}
